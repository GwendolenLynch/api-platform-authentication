{
    "name": "camelot/api-authentication",
    "description": "Authentication bundle for API Platform",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gwendolen Lynch",
            "email": "gwendolen.lynch@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "api-platform/core": "^2.7 || ^3.0",
        "gesdinet/jwt-refresh-token-bundle": "^1.1",
        "lexik/jwt-authentication-bundle": "^2.16",
        "nelmio/cors-bundle": "^2.2",
        "doctrine/orm": "^2.12",
        "symfony/security-bundle": "^6.2.x-dev@dev",
        "symfony/console": "^6.1"
    },
    "require-dev": {
        "camelot/coding-style": "^3.0",
        "dama/doctrine-test-bundle": "^7.1",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.7",
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.1",
        "symfony/dotenv": "^6.1",
        "symfony/http-client": "^6.1",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/runtime": "^6.1",
        "symfony/yaml": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Camelot\\Api\\Authentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Camelot\\Api\\Authentication\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
